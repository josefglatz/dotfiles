# https://docs.github.com/en/free-pro-team@latest/github/authenticating-to-github/generating-a-new-ssh-key-and-adding-it-to-the-ssh-agent#adding-your-ssh-key-to-the-ssh-agent
Host *
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_rsa

# Statically integrated host configurations are stored here





Match host "*"
# Dynamically added host configurations are added here (with Include statements)
# Example: "Include /Volumes/CS/Projects/Customer-A/WebsiteOfCustomer-A/.config/ssh_config"
